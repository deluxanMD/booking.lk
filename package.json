  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverageold": "CI=true npm test -- --env=jsdom --watchAll=false --coverage",
    "cy:run": "cypress run --e2e",
    "ct:run": "cypress run --component",
    "cy:open": "cypress open --e2e",
    "ct:open": "cypress open --component",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:watch": "npm run test -- --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint": "npx eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier:check": "npx prettier --check src",
    "prettier:fix": "npx prettier --write src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/instrument-cra": "^1.4.0",
    "@hookform/devtools": "^4.3.1",
    "@testing-library/cypress": "^10.0.1",
    "@types/cypress": "^1.1.3",
    "cypress": "^13.6.0",
