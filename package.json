    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.ts",
      "<rootDir>/src/reportWebVitals.ts",
      "<rootDir>/.*.cy.tsx$"
    ]
  }
}
